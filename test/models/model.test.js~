define(["models","assert"],function(model,assert){

  describe('Models', function() {
    describe('Sample Model', function() {
      var sample = new model();

      it('should have default "name" property to "zzz"', function() {
        assert(sample.name != 'zzz'," default name not equal to zzz");
      });

      it('should have default "author" property to "Z Zz"', function() {
        assert(sample.author != 'Z Zz'," default author not equal to Z Zz");
      });

      it('should default "category" property to "zz"', function() {
        assert(sample.category != 'zz'," default category not equal to zz");
      });

      it('should default "description" property to "zzzzz"', function() {
        assert(sample.description != 'zzzzzzz'," default description not equal to zzzzz");
      });

      it('should default "url" property to [blank]', function() {  
        assert(sample.url != ''," default category not equal to [blank]");
      });

    });
   });

});
